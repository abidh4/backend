
const mongoose = require("mongoose");
const mySchema = mongoose.Schema({
  studentName: {
    type: String,
    required: true,
  },
  fathersName: {
    type: String,
    required: true,
  },
  mothersName: {
    type: String,
    required: true,
  },
  studentSession: { type: String, required: true },
  deptName: {
    type: String,
    required: true,
  },
  gender: {
    type: String,
    required: true,
  },
  contactNo: {
    type: String,
    required: true,
  },
  eMail: {
    type: String,
    required: true,
  },
  address: {
    type: String,
    required: true,
  },
  meritPosition: {
    type: Number,
    required: true
  },

});
module.exports = mongoose.model("admission-candidate-list", mySchema);